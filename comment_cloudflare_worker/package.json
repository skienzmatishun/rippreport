{
  "name": "cloudflare-comment-system",
  "version": "1.0.0",
  "description": "Anonymous comment system built on Cloudflare Workers and KV storage",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "deploy:development": "wrangler deploy --env development",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "comments",
    "hugo",
    "serverless"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240909.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "vitest": "^1.0.0",
    "wrangler": "^3.78.0"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}